{
  "name": "adfs-test-pureclient",
  "version": "1.0.0",
  "description": "Pure clientside auth test",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "dev": "webpack --mode development --watch",
    "devbuild": "webpack --mode development",
    "build": "webpack --mode production",
    "lint": "prettier --check app __tests__",
    "lint:fix": "prettier --write app __tests__"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "tsx",
      "ts",
      "json"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.[jt]sx?$": "ts-jest",
      "node_modules/react-multistep/.+\\.js$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-multistep)"
    ],
    "testResultsProcessor": "jest-junit",
    "setupFiles": [
      "./jestSetup.jsx"
    ]
  },
  "author": "Andy Gallagher <andy.gallagher@theguardian.com>",
  "devDependencies": {
    "@teamsupercell/typings-for-css-modules-loader": "^2.4.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/sinon": "^9.0.4",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "expect": "^24.9.0",
    "husky": ">=4",
    "jest": "^26.0.1",
    "jest-cli": "^24.8.0",
    "jest-fetch-mock": "^3.0.1",
    "jest-junit": "^4.0.0",
    "lint-staged": ">=10",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.12.0",
    "sinon": "^2.3.7",
    "source-map-loader": "^1.0.0",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^7.0.5",
    "typescript": "^4.2.3",
    "util": "^0.12.3"
  },
  "dependencies": {
    "@guardian/pluto-headers": "^2.0.0-pre6",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "4.0.0-alpha.58",
    "ajv": "^6.12.3",
    "axios": "^1.6.4",
    "cross-storage": "^1.0.0",
    "css-loader": "^5.2.6",
    "date-fns": "^2.22.1",
    "identity-obj-proxy": "^3.0.0",
    "jose": "^4.8.1",
    "process": "^0.11.10",
    "query-string": "^6.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "y18n": "^4.0.1"
  },
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  },
  "resolutions": {
    "ajv": "^6.12.3",
    "y18n": "^4.0.1"
  }
}
